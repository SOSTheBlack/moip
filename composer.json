{
    "name": "sostheblack/moip",
    "description": "Laravel Package for Moip.",
    "keywords": ["moip", "gateway", "payment", "laravel", "checkout"],
    "license": "MIT",
    "type": "libraries",
    "authors": [
        {
            "name": "Jean C. Garcia",
            "email": "jeancesargarcia@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*|5.*",
        "illuminate/html": "4.*|5.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/commands",
            "src/models",
            "src/seeds"
        ],
        "psr-0": {
            "SOSTheBlack\\Moip\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "minimum-stability": "stable"
}
